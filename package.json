{
  "name": "react-query-crud",
  "version": "0.1.9",
  "license": "MIT",
  "author": "Sergey Shpadyrev",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "tabWidth": 4,
    "trailingComma": "all"
  },
  "module": "dist/react-query-lists.esm.js",
  "size-limit": [
    {
      "path": "dist/react-query-lists.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-query-lists.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "husky": "^9.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "size-limit": "^11.1.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.52.1"
  }
}
